@using Cv.Mvc.Project.Models
@model List<TblAbout>
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>CV Projesi</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="Free HTML Templates" name="keywords">
    <meta content="Free HTML Templates" name="description">

    <!-- Favicon -->
    <link href="/Template/CvTemp/img/favicon.ico" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700;900&display=swap" rel="stylesheet">

    <!-- Font Awesome -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="/Template/CvTemp/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="/Template/CvTemp/lib/lightbox/css/lightbox.min.css" rel="stylesheet">

    <!-- Customized Bootstrap Stylesheet -->
    <link href="/Template/CvTemp/css/style.css" rel="stylesheet">
</head>

<body data-spy="scroll" data-target=".navbar" data-offset="51">
    <!-- Navbar Start -->
    <nav class="navbar fixed-top shadow-sm navbar-expand-lg bg-light navbar-light py-3 py-lg-0 px-lg-5">
        <a href="index.html" class="navbar-brand ml-lg-3">
            <h1 class="m-0 display-5"><span class="text-primary">CV</span>Projesi</h1>
        </a>
        <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#navbarCollapse">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse px-lg-3" id="navbarCollapse">
            <div class="navbar-nav m-auto py-0">
                <a href="#home" class="nav-item nav-link active">Anasayfa</a>
                <a href="#about" class="nav-item nav-link">Hakkımda</a>
                <a href="#qualification" class="nav-item nav-link">Eğitimler</a>
                <a href="#skill" class="nav-item nav-link">Yetenekler</a>
                <a href="#service" class="nav-item nav-link">Projelerim</a>
                <a href="#testimonial" class="nav-item nav-link">Referanslar</a>
                <a href="#contact" class="nav-item nav-link">İletişim</a>
            </div>
        </div>
    </nav>
    <!-- Navbar End -->
    <!-- Video Modal Start -->
    <div class="modal fade" id="videoModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-body">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <!-- 16:9 aspect ratio -->
                    <div class="embed-responsive embed-responsive-16by9">
                        <iframe class="embed-responsive-item" src="" id="video" allowscriptaccess="always" allow="autoplay"></iframe>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Video Modal End -->
    <!-- Header Start -->
    <div class="container-fluid bg-primary d-flex align-items-center mb-5 py-5" id="home" style="min-height: 100vh;">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-5 px-5 pl-lg-0 pb-5 pb-lg-0">
                    <img class="img-fluid w-100 rounded-circle shadow-sm" src="/Template/CvTemp/img/Gizem.jpg" alt="">
                </div>
                <div class="col-lg-7 text-center text-lg-left">
                    <h3 class="text-white font-weight-normal mb-3"></h3>
                    <h1 class="display-3 text-uppercase text-primary mb-2" style="-webkit-text-stroke: 2px #ffffff;">Gizem Emniyetli</h1>
                    <h1 class="typed-text-output d-inline font-weight-lighter text-white"></h1>
                    <div class="typed-text d-none">Jr. .Net Developer</div>
                    <div class="d-flex align-items-center justify-content-center justify-content-lg-start pt-5">
                        <a href="" class="btn btn-outline-light mr-5">CV İndir</a>
                        <button type="button" class="btn-play" data-toggle="modal"
                                data-src="https://www.youtube.com/embed/DWRcNpR6Kdc" data-target="#videoModal">
                            <span></span>
                        </button>
                        <h5 class="font-weight-normal text-white m-0 ml-4 d-none d-sm-block">Play Video</h5>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Header End -->
    <!-- About Start -->
    <div class="container-fluid py-5" id="about">
        @foreach (var item in Model)
        {
            <div class="container">
                <div class="position-relative d-flex align-items-center justify-content-center">
                    <h1 class="display-1 text-uppercase text-white" style="-webkit-text-stroke: 1px #dee2e6;">Hakkımda</h1>
                    <h1 class="position-absolute text-uppercase text-primary">Hakkımda</h1>
                </div>
                <div class="row align-items-center">
                    <div class="col-lg-5 pb-4 pb-lg-0">
                        <img class="img-fluid rounded w-100" src="/Template/CvTemp/img/Gizem.jpg" alt="">
                    </div>
                    <div class="col-lg-7">
                        <h3 class="mb-4">Jr. .Net Developer</h3>
                        <p>@item.Açıklama</p>
                        <div class="row mb-3">
                            <div class="col-sm-6 py-2"><h6>Ad-Soyad: <span class="text-secondary">@item.Ad @item.Soyad</span></h6></div>
                            <div class="col-sm-6 py-2"><h6>Doğum Tarihi: <span class="text-secondary">27/07/1993</span></h6></div>
                            <div class="col-sm-6 py-2"><h6>Derece: <span class="text-secondary">Yüksek Lisans</span></h6></div>
                            <div class="col-sm-6 py-2"><h6>Telefon: <span class="text-secondary">@item.Telefon</span></h6></div>
                            <div class="col-sm-6 py-2"><h6>Email: <span class="text-secondary">@item.Mail</span></h6></div>
                            <div class="col-sm-6 py-2"><h6>Address: <span class="text-secondary">@item.Adres</span></h6></div>
                        </div>

                    </div>
                </div>
            </div>

        }
    </div>
    <!-- About End -->
    <!-- Qualification Start -->
    @Html.Action("Education")
    <!-- Qualification End -->
    <!-- Skill Start -->
    @Html.Action("Skills")
    <!-- Skill End -->
    <!-- Services Start -->
    @Html.Action("MyProject")
    <!-- Services End -->
    <!-- Portfolio Start -->
  
    <!-- Portfolio End -->
    <!-- Testimonial Start -->
    @Html.Action("MyReferans")
    <!-- Testimonial End -->
    <!-- Blog Start -->
   
    <!-- Blog End -->
    <!-- Contact Start -->
    @Html.Action("Contact")
    <!-- Contact End -->
    <!-- Footer Start -->
    <div class="container-fluid bg-primary text-white mt-5 py-5 px-sm-3 px-md-5">
        <div class="container text-center py-5">
            <div class="d-flex justify-content-center mb-4">
                <a class="btn btn-light btn-social mr-2" href="https://github.com/Sun-Leo"><i class="fab fa-github"></i></a>
                <a class="btn btn-light btn-social mr-2" href="https://www.linkedin.com/in/gizem-emniyetli-3a3b16225/"><i class="fab fa-linkedin-in"></i></a>
                <a class="btn btn-light btn-social" href="#"><i class="fab fa-instagram"></i></a>
            </div>
            <div class="d-flex justify-content-center mb-3">
                <a class="text-white" href="#">Privacy</a>
                <span class="px-3">|</span>
                <a class="text-white" href="#">Terms</a>
                <span class="px-3">|</span>
                <a class="text-white" href="#">FAQs</a>
                <span class="px-3">|</span>
                <a class="text-white" href="#">Help</a>
            </div>
            <p class="m-0">
                &copy; <a class="text-white font-weight-bold" href="#"></a>. Gizem Emniyetli 
        </div>
    </div>
    <!-- Footer End -->
    <!-- Scroll to Bottom -->
    <i class="fa fa-2x fa-angle-down text-white scroll-to-bottom"></i>

    <!-- Back to Top -->
    <a href="#" class="btn btn-outline-dark px-0 back-to-top"><i class="fa fa-angle-double-up"></i></a>


    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"></script>
    <script src="/Template/CvTemp/lib/typed/typed.min.js"></script>
    <script src="/Template/CvTemp/lib/easing/easing.min.js"></script>
    <script src="/Template/CvTemp/lib/waypoints/waypoints.min.js"></script>
    <script src="/Template/CvTemp/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="/Template/CvTemp/lib/isotope/isotope.pkgd.min.js"></script>
    <script src="/Template/CvTemp/lib/lightbox/js/lightbox.min.js"></script>

    <!-- Contact Javascript File -->
    <script src="/Template/CvTemp/mail/jqBootstrapValidation.min.js"></script>
    <script src="/Template/CvTemp/mail/contact.js"></script>

    <!-- Template Javascript -->
    <script src="/Template/CvTemp/js/main.js"></script>
</body>

</html>
